{
    "exclude": [
        "build",
        "dist",
        ".venv",
        "tests",
        "test_requests",
        "temp",
    ],
    "reportMissingImports": true,
    "reportUnusedImport": "warning",
    "typeCheckingMode": "basic",
    "ignore": [
        "reportImplicitOverride",
        "reportRedeclaration"
    ],
    "reportImplicitOverride": false
}